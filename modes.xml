<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="ukr-morph">
    <pipeline>
      <program name="lt-proc -w">
        <file name="ukr.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ukr-gener">
    <pipeline>
      <program name="lt-proc -g">
        <file name="ukr.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ukr-tagger">
    <pipeline>
      <program name="lt-proc -w">
        <file name="ukr.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="ukr.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="ukr-disam">
    <pipeline>
      <program name="lt-proc -w">
        <file name="ukr.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-ukr.ukr.rlx"/>
      </program>
    </pipeline>
  </mode>


</modes>
